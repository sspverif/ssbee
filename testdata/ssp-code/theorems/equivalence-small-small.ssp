theorem SmallIsSmall {
  const n: Integer;

  instance smallA = SmallGame{
    params {
      n: n 
    }
  }

  instance smallB = SmallGame{
    params {
      n: n 
    }
  }

  gamehops {
    equivalence smallA smallB {
      N: {
        invariant: ./invariant.smt
        lemmas {
        smt_ident: []
        }
      }
    }
  }
}
